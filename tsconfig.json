{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "baseUrl": "/",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      }
    },
    "include": [
        "src/**/*"
    ]
  }